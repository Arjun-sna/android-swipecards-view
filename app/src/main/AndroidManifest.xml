        <action android:name="android.intent.action.MAIN"/>
        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity android:name=".CardSwipeWithFragment">
    </activity>
    <activity
        android:name=".PageSwipeActivity"
        android:label="@string/app_name">
    </activity>
  </application>

</manifest>
