      android:label="@string/app_name"
      android:theme="@style/AppTheme">
    <activity
        android:name=".CardSwipeActivity"
        android:label="@string/app_name"></activity>
    <activity
        android:name=".HomeActivity"
        android:label="@string/app_name">
